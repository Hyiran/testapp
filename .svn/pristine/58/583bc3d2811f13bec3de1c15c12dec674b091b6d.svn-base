	/**
	 * 判断取号列是否存在
	 * @param month
	 * @param networkType
	 * @return
	 * @throws SQLException
	 */
	public static boolean isExistOrder(String month,String networkType) throws SQLException{
		boolean isExist = false;
		String sqlExist = "select count(1)  as isExist from tb_app_order where month = ? and networkType = ?";
		String[] parameters = { month,networkType};
		ResultSet rs = SqlHelper.executeQuery(sqlExist, parameters);
		while(rs.next()){
			if(!rs.getString(1).toString().equals("0")){
				isExist = true;
			}
		}
		return isExist;
	}
	
