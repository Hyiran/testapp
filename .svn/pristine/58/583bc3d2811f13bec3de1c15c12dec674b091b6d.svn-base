	public static void updateLog(String productName,String type,String networkType){
		String field = "";
		
		switch(productName.toUpperCase()){
			case "139":field = type+"139";
			           break;
			case "163":field =  type+"163";
	                   break;
			case "QQ":field =  type+"qq";
	                  break;
			case "189":field =  type+"189";
	                   break;
	        default:break;
		}
		String sql = "update tb_app_log set "+field+" = 'Y' where pcName = ? and networkType = ? order by id desc LIMIT 1";
		String[] parameters = {CommonUtil.getName(),networkType};
		SqlHelper.executeUpdate(sql, parameters);
	}
	
